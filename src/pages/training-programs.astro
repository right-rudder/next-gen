---
import BaseLayout from "../layouts/BaseLayout.astro";
import trainingPrograms from "../data/training-programs";
import HeaderScreen from "../components/HeaderScreen.astro";
import ProgramsSwiper from "../components/ProgramsSwiper.astro";

const { data } = trainingPrograms;
---

<BaseLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <!-- HEADER -->
  <HeaderScreen content={data.header} />

  <ProgramsSwiper content={data.programs} />
</BaseLayout>
<script src="../scripts/widerSlider.js"></script>
